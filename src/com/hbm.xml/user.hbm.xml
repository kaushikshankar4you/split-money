<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.entity.User" table="user">
		<id name="id" column="id" type="int">
			<generator class="increment" />
		</id>
		<property name="phone" column="phone" not-null="true" length="20"/>
		<property name="name" column="name" not-null="true" length="20"/>
		<property name="password" column="password" not-null="true" length="16" />
		<property name="wallet_id" column="wallet_id" not-null="true" length="16" />

	</class>

</hibernate-mapping>
